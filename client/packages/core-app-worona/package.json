  "scripts": {
    "clean": "rimraf dist",
    "clean:dev": "rimraf dist/dev",
    "clean:prod": "rimraf dist/prod",
    "clean:core:dev": "rimraf dist/dev/js && rimraf dist/dev/locales && rimraf dist/dev/html",
    "clean:core:prod": "rimraf dist/prod/js && rimraf dist/prod/locales && rimraf dist/prod/html",
    "clean:vendors:dev": "rimraf dist/dev/vendors",
    "clean:vendors:prod": "rimraf dist/prod/vendors",
    "build:core:dev": "npm run clean:core:dev && webpack --progress --config webpack.dev.js",
    "build:core:prod": "npm run clean:core:prod && webpack -p --progress --config webpack.prod.js",
    "build:core:dev:modules": "npm run clean:core:dev && webpack --progress --config webpack.dev.js --display-modules",
    "build:core:prod:modules": "npm run clean:core:prod && webpack -p --progress --config webpack.prod.js --display-modules",
    "build:core:dev:reasons": "npm run clean:core:dev && webpack --progress --config webpack.dev.js --display-modules --display-reasons",
    "build:core:prod:reasons": "npm run clean:core:prod && webpack -p --progress --config webpack.prod.js --display-modules --display-reasons",
    "build:core:dev:stats": "npm run clean:core:dev && webpack --progress --config webpack.dev.js --profile --json > dev-stats.json",
    "build:core:prod:stats": "npm run clean:core:prod && webpack -p --progress --config webpack.prod.js --profile --json > prod-stats.json",
    "build:core": "npm run build:core:dev && npm run build:core:prod",
    "build:vendors:dev": "npm run clean:vendors:dev && webpack --progress --config vendors/webpack.dev.js",
    "build:vendors:prod": "npm run clean:vendors:prod && webpack -p --progress --config vendors/webpack.prod.js",
    "build:vendors:dev:modules": "npm run clean:vendors:dev && webpack --progress --config vendors/webpack.dev.js --display-modules",
    "build:vendors:prod:modules": "npm run clean:vendors:prod && webpack --progress --config vendors/webpack.prod.js --display-modules",
    "build:vendors:dev:reasons": "npm run clean:vendors:dev && webpack -p --progress --config vendors/webpack.dev.js --display-modules --display-reasons",
    "build:vendors:prod:reasons": "npm run clean:vendors:prod && webpack -p --progress --config vendors/webpack.prod.js --display-modules --display-reasons",
    "build:vendors:dev:stats": "npm run clean:vendors:dev && webpack -p --progress --config vendors/webpack.dev.js --profile --json > vendors/dev-stats.json",
    "build:vendors:prod:stats": "npm run clean:vendors:prod && webpack -p --progress --config vendors/webpack.prod.js --profile --json > vendors/prod-stats.json",
    "build:vendors": "npm run build:vendors:dev && npm run build:vendors:prod",
    "build": "npm run build:vendors && npm run build:core"
  },
